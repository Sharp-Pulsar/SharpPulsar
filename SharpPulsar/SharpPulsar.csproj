<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>1.3.5</Version>
    <Authors>Ebere Abanonu</Authors>
    <Description>SharpPulsar is Apache Pulsar Client built using Akka.net</Description>
    <PackageTags>Apache Pulsar, Akka.Net</PackageTags>
    <PackageReleaseNotes>More info to Consumer connected logging</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/eaba/SharpPulsar</RepositoryUrl>
    <PackageProjectUrl>https://github.com/eaba/SharpPulsar</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <TargetsForTfmSpecificBuildOutput>$(TargetsForTfmSpecificBuildOutput);CopyProjectReferencesToPackage</TargetsForTfmSpecificBuildOutput>
    <PackageIcon>SharpPulsar.jpg</PackageIcon>
    <PackageIconUrl />
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Akka\InternalCommands\Reader\**" />
    <Compile Remove="Protocol\Circe\**" />
    <Compile Remove="Protocol\Proto\Partial\**" />
    <EmbeddedResource Remove="Akka\InternalCommands\Reader\**" />
    <EmbeddedResource Remove="Protocol\Circe\**" />
    <EmbeddedResource Remove="Protocol\Proto\Partial\**" />
    <None Remove="Akka\InternalCommands\Reader\**" />
    <None Remove="Protocol\Circe\**" />
    <None Remove="Protocol\Proto\Partial\**" />
    <None Include="SharpPulsar.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Impl\Schema\GenerateSchema.cs" />
    <Compile Remove="Impl\Schema\KeyValueSchema.cs" />
    <Compile Remove="Impl\Schema\KeyValueSchemaInfo.cs" />
    <Compile Remove="Presto\FixJsonDataUtils.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="akka" Version="1.4.9" />
    <PackageReference Include="Akka.Logger.NLog" Version="1.4.5" />
    <PackageReference Include="Apache.Avro" Version="1.10.0" />
    <PackageReference Include="App.Metrics.Concurrency" Version="2.0.2" />
    <PackageReference Include="avroschemagenerator" Version="1.6.0" />
    <PackageReference Include="BeetleX" Version="1.5.2.6" />
    <PackageReference Include="DotNetty.Common" Version="0.6.0" />
    <PackageReference Include="Google.Protobuf" Version="3.12.4" />
    <PackageReference Include="HttpToSocks5Proxy" Version="1.4.0" />
    <PackageReference Include="IdentityModel" Version="4.3.1" />
    <PackageReference Include="JsonSubTypes" Version="1.7.0" />
    <PackageReference Include="K4os.Compression.LZ4" Version="1.1.11" />
    <PackageReference Include="Mail.dll" Version="3.0.20174.2011" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="3.1.6" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="3.1.6" />
    <PackageReference Include="Microsoft.Rest.ClientRuntime" Version="2.3.21" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Nito.AsyncEx.Tasks" Version="5.0.0" />
    <PackageReference Include="NodaTime" Version="3.0.0" />
    <PackageReference Include="Portable.BouncyCastle" Version="1.8.6.7" />
    <PackageReference Include="protobuf-net" Version="3.0.29" />
    <PackageReference Include="RestSharp" Version="106.11.4" />
    <PackageReference Include="SharpZipLib" Version="1.2.0" />
    <PackageReference Include="Snappy.Standard" Version="0.2.0" />
    <PackageReference Include="System.Diagnostics.Contracts" Version="4.3.0" />
    <PackageReference Include="System.IO.Pipelines" Version="4.7.2" />
    <PackageReference Include="System.Net.NetworkInformation" Version="4.3.0" />
    <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="4.7.1" />
    <PackageReference Include="System.Runtime.Serialization.Primitives" Version="4.3.0" />
    <PackageReference Include="System.Text.Json" Version="4.7.2" />
    <PackageReference Include="zlib.net-mutliplatform" Version="1.0.4" />
    <PackageReference Include="ZstdNet" Version="1.3.3" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Akka\Function\Api\Swagger.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Swagger.json</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Akka\Admin\Api\code-model-v1">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Akka\Function\Api\Swagger.json">
      <Generator>NSwagCodeGenerator</Generator>
      <LastGenOutput>Swagger.cs</LastGenOutput>
    </None>
    <None Update="NLog.config">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="readme.txt" pack="true" PackagePath=".">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Presto\Facebook\Type\zone-index.properties">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="SharpPulsar.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <Target DependsOnTargets="ResolveReferences" Name="CopyProjectReferencesToPackage">
    <ItemGroup>
      <BuildOutputInPackage Include="@(ReferenceCopyLocalPaths-&gt;WithMetadataValue('ReferenceSourceTarget', 'ProjectReference'))" />
    </ItemGroup>
  </Target>
  <ProjectExtensions><VisualStudio><UserProperties akka_4function_4api_4swagger_1json__AvroSchema="" /></VisualStudio></ProjectExtensions>
</Project>
